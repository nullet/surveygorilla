<h3><%= @survey.title %></h3>

<ol>
  <% @survey.questions.each do |question| %>
    <li><%= question.text %></li> 
    <ul>
      <% question.choices.each do |choice| %>
            <li><%= choice.text %>: <%= to_percent(choice.answers.length, @total) %>%</li>
      <% end %>
    </ul>
  <% end %>
</ol>